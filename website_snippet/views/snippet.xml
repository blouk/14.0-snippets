<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <template id="snippets" inherit_id="website.snippets">
        <xpath expr="//div[@id='snippet_structure']" position="before">
            <div id="snippet_structure" class="o_panel">
                <div class="o_panel_header">My Snippets</div>
                <div class="o_panel_body">
                    <t t-snippet="website_snippet.s_dynamic_snippet_products_selector" t-thumbnail="/website_snippet/static/src/snippet/s_dyn_products_selector/s_thumb.jpg">
                        <keywords>Product Selector</keywords>
                    </t>
                </div>
            </div>
        </xpath>
    </template>
</odoo>
